@article{guechichiAnalyticalModelRelate2013,
  title = {An {{Analytical Model}} to {{Relate Shot Peening Almen Intensity}} to {{Shot Velocity}}},
  author = {Guechichi, H. and Castex, L. and Benkhettab, M.},
  year = {2013},
