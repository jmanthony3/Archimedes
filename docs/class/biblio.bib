@article{chenLiteratureReviewNumerical2019,
  title = {Literature Review of Numerical Simulation and Optimisation of the Shot Peening Process},
  author = {Chen, Jun S and Desai, Dawood A and Heyns, Stephan P and Pietra, Francesco},
  year = {2019},
  month = mar,
  journal = {Advances in Mechanical Engineering},
  volume = {11},
  number = {3},
  pages = {1687814018818277},
